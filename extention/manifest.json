{
    "manifest_version": 3,
    "name": "Notification Extension",
    "version": "1.0",
    "description": "Displays a notification when the button is clicked",
    "permissions": [
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}